      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "sass variables",
      "scope": ["sass.script.maps", "scss.script.maps"],
      "settings": {
        "foreground": "#e28087"
      }
    },
    {
      "name": "sass function interpolation",
