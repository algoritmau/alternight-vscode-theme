{
  "name": "alternight",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    // Editor
    "editor.background": "#1f172b",
    "editor.foreground": "#d5d5d5",
    "editor.selectionBackground": "#ffff0048",
    "editorCursor.foreground": "#ff0",
    "editorLineNumber.foreground": "#9077b688",
    "editor.lineHighlightBackground": "#00000064",

    "titleBar.activeBackground": "#1f172b",
    "titleBar.activeForeground": "#9077b6",
    "titleBar.inactiveBackground": "#1f172b80",
    "titleBar.inactiveForeground": "#9077b680",

    // Activity bar
    "activityBar.background": "#1f172b",
    "activityBar.foreground": "#bcadd3",
    "activityBar.activeBorder": "#bcadd3",
    "activityBar.inactiveForeground": "#9077b680",
    "activityBarBadge.background": "#e29fa4",
    "activityBarBadge.foreground": "#1f172b",

    // Sidebar
    "sideBar.background": "#1f172b",
    "sideBar.foreground": "#9077b6",
    "sideBarTitle.foreground": "#bcadd3",
    "sideBarSectionHeader.background": "#312a3c",
    "sideBarSectionHeader.foreground": "#9b8eb0",

    // Tabs
    "editorGroupHeader.tabsBackground": "#1f172b",
    "tab.activeBorderTop": "#f78c6c",
    "tab.activeForeground": "#f78c6c",
    "tab.inactiveBackground": "#1b1b31",
    "tab.inactiveForeground": "#7F7fb8",
    "tab.hoverBackground": "#f78c6c",
    "tab.hoverForeground": "#1f172b",

    // Status bar
    "statusBar.background": "#1f172b",
    "statusBar.foreground": "#9077b6",
    "statusBarItem.hoverBackground": "#14141472",
    "statusBar.debuggingBackground": "#e29fa4",
    "statusBar.debuggingForeground": "#1f172b",

    // Integrated Terminal
    "terminal.background": "#1f172b",
    "terminal.foreground": "#d5d5d5",
    "terminal.selectionBackground": "#10101099",
    "terminalCursor.foreground": "#ff0",

    // Menu
    "menu.background": "#1f172b",
    "menu.foreground": "#c6abde",
    "menu.border": "#c6abde",
    "menu.selectionBackground": "#fefefe40",
    "menu.separatorBackground": "#fefefe32",
    "window.activeBorder": "#9292e424",

    "editor.hoverHighlightBackground": "#695d5e",
    "editorIndentGuide.background": "#312a3c",
    "editorIndentGuide.activeBackground": "#797480",

    // Hover Widget
    "editorHoverWidget.background": "#312a3c",
    "editorHoverWidget.border": "#c6abde",
    "editorHoverWidget.foreground": "#c6abde",

    "editorGutter.addedBackground": "#9fff20",
    "editorGutter.foldingControlForeground": "#ff0",

    // Misc
    "scrollbar.shadow": "#d5d5d508"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "comment.block.documentation",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#b37895"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "punctuation.definition.string.template.begin.js",
        "punctuation.definition.string.template.begin.jsx",
        "punctuation.definition.string.template.begin.ts",
        "punctuation.definition.string.template.begin.tsx",
        "punctuation.definition.string.template.end.js",
        "punctuation.definition.string.template.end.jsx",
        "punctuation.definition.string.template.end.ts",
        "punctuation.definition.string.template.end.tsx"
      ],
      "settings": {
        "foreground": "#2ba769"
      }
    },
    {
      "name": "Template Expression",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.begin.jsx",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.begin.tsx",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.end.jsx",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.template-expression.end.tsx"
      ],
      "settings": {
        "foreground": "#b5bcff"
      }
    },
    {
      "name": "HTML Entities",
      "scope": [
        "constant.character.entity.js",
        "constant.character.entity.jsx",
        "constant.character.entity.ts",
        "constant.character.entity.tsx"
      ],
      "settings": {
        "foreground": "#43c2e2"
      }
    },
    {
      "name": "Punctuation of HTML Entities",
      "scope": [
        "punctuation.definition.entity.js",
        "punctuation.definition.entity.jsx",
        "punctuation.definition.entity.ts",
        "punctuation.definition.entity.tsx"
      ],
      "settings": {
        "foreground": "#3896ad"
      }
    },
    {
      "name": "TS Tag Directives",
      "scope": [
        "entity.name.tag.directive.js",
        "entity.name.tag.directive.jsx",
        "entity.name.tag.directive.ts",
        "entity.name.tag.directive.tsx"
      ],
      "settings": {
        "foreground": "#f0bb6b"
      }
    },
    {
      "name": "Punctuation of TS Tag Directives",
      "scope": [
        "punctuation.definition.tag.directive.js",
        "punctuation.definition.tag.directive.jsx",
        "punctuation.definition.tag.directive.ts",
        "punctuation.definition.tag.directive.tsx"
      ],
      "settings": {
        "foreground": "#ac66c9"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#fefefe"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c158eb"
      }
    },
    {
      "name": "Entity HTML Tags",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#d4785d"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#ac66c9"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#c7c7df"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#5d2cb9"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#e7d5a4"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#df84a2"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#e0a1a4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#b1bdbd",
        "fontStyle": ""
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "meta.group.braces.curly constant.other.object.key.jsx string.unquoted.label.jsx",
        "meta.group.braces.curly constant.other.object.key.ts string.unquoted.label.ts",
        "meta.group.braces.curly constant.other.object.key.tsx string.unquoted.label.tsx"
      ],
      "settings": {
        "foreground": "#2ba769"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#f0bb6b"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#b2ccd6"
      }
    },
    {
      "name": "CSS at-rules",
      "scope": [
        "keyword.control.at-rule.css",
        "keyword.control.at-rule.sass",
        "keyword.control.at-rule.scss",
        "keyword.control.at-rule.use",
        "support.function.name.sass.library",
        "support.function.name.scss.library"
      ],
      "settings": {
        "foreground": "#c2a0fd",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS/sass/scss functions",
      "scope": [
        "support.function.name.sass.library",
        "support.function.name.scss.library"
      ],
      "settings": {
        "foreground": "#d153bc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Variable",
      "scope": ["variable.css"],
      "settings": {
        "foreground": "#c7c7df"
      }
    },
    {
      "name": "CSS String",
      "scope": [
        "constant.character.css",
        "constant.character.css.sass",
        "constant.character.css.scss"
      ],
      "settings": {
        "foreground": "#2ba769"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "entity.name.module.jsx",
        "entity.name.module.ts",
        "entity.name.module.tsx",
        "variable.import.parameter.js",
        "variable.import.parameter.jsx",
        "variable.import.parameter.ts",
        "variable.import.parameter.tsx",
        "variable.other.class.js",
        "variable.other.class.jsx",
        "variable.other.class.ts",
        "variable.other.class.tsx"
      ],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#ff6781"
      }
    },
    {
      "name": "JS trycatch",
      "scope": [
        "keyword.control.trycatch.js",
        "keyword.control.trycatch.jsx",
        "keyword.control.trycatch.ts",
        "keyword.control.trycatch.tsx"
      ],
      "settings": {
        "foreground": "#a8b0eb",
        "fontStyle": "bold"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js",
        "entity.name.method.jsx",
        "entity.name.method.ts",
        "entity.name.method.tsx"
      ],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "Variable Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#8ebcd6"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "meta.class-method.jsx entity.name.function.jsx",
        "meta.class-method.ts entity.name.function.ts",
        "meta.class-method.tsx entity.name.function.tsx",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "Constant Language",
      "scope": "constant.language",
      "settings": {
        "foreground": "#40cfcf"
      }
    },
    {
      "name": "null/undefined Keywords JS",
      "scope": [
        "constant.language.null.js",
        "constant.language.null.jsx",
        "constant.language.null.ts",
        "constant.language.null.tsx",
        "constant.language.undefined.js",
        "constant.language.undefined.jsx",
        "constant.language.undefined.ts",
        "constant.language.undefined.tsx"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object Keys Keyword JS",
      "scope": [
        "meta.object-literal.key.js",
        "meta.object-literal.key.jsx",
        "meta.object-literal.key.ts",
        "meta.object-literal.key.tsx",
        "meta.object.member.js",
        "meta.object.member.jsx",
        "meta.object.member.ts",
        "meta.object.member.tsx",
        "meta.objectliteral.js",
        "meta.objectliteral.jsx",
        "meta.objectliteral.ts",
        "meta.objectliteral.tsx"
      ],
      "settings": {
        "foreground": "#e8f3f3"
      }
    },
    // JSX Styles
    {
      "name": "JSX Tag Name",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.jsx",
        "entity.name.tag.ts",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#d4785d"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property.js",
        "variable.other.property.jsx",
        "variable.other.property.ts",
        "variable.other.property.tsx"
      ],
      "settings": {
        "foreground": "#5c7fe2"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other.readwrite.alias.js",
        "variable.other.readwrite.alias.jsx",
        "variable.other.readwrite.alias.ts",
        "variable.other.readwrite.alias.tsx"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "false/true Keywords",
      "scope": [
        "constant.language.boolean.false.js",
        "constant.language.boolean.false.jsx",
        "constant.language.boolean.false.ts",
        "constant.language.boolean.false.tsx",
        "constant.language.boolean.true.js",
        "constant.language.boolean.true.jsx",
        "constant.language.boolean.true.ts",
        "constant.language.boolean.true.tsx"
      ],
      "settings": {
        "foreground": "#40cfcf",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Conditional Keywords",
      "scope": [
        "keyword.control.conditional.js",
        "keyword.control.conditional.jsx",
        "keyword.control.conditional.ts",
        "keyword.control.conditional.tsx",
        "keyword.control.switch.js",
        "keyword.control.switch.jsx",
        "keyword.control.switch.ts",
        "keyword.control.switch.tsx"
      ],
      "settings": {
        "foreground": "#41c09a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Return Keyword",
      "scope": [
        "keyword.control.flow.js",
        "keyword.control.flow.jsx",
        "keyword.control.flow.ts",
        "keyword.control.flow.tsx"
      ],
      "settings": {
        "foreground": "#e27171",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Export, Default Keywords",
      "scope": [
        "keyword.control.export.js",
        "keyword.control.export.jsx",
        "keyword.control.export.ts",
        "keyword.control.export.tsx",
        "keyword.control.default.js",
        "keyword.control.default.jsx",
        "keyword.control.default.ts",
        "keyword.control.default.tsx"
      ],
      "settings": {
        "foreground": "#e4baba"
      }
    },
    {
      "name": "JSX Tags",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.jsx",
        "entity.name.tag.ts",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#d4785d"
      }
    },
    {
      "name": "JSX Meta",
      "scope": ["meta.tag.js", "meta.tag.jsx", "meta.tag.ts", "meta.tag.tsx"],
      "settings": {
        "foreground": "#d5d5d5"
      }
    },
    {
      "name": "JSX Tag Punctuation",
      "scope": [
        "punctuation.definition.tag.begin.js",
        "punctuation.definition.tag.begin.jsx",
        "punctuation.definition.tag.begin.ts",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.js",
        "punctuation.definition.tag.end.jsx",
        "punctuation.definition.tag.end.ts",
        "punctuation.definition.tag.end.tsx"
      ],
      "settings": {
        "foreground": "#ac66c9"
      }
    },
    {
      "name": "Import JS",
      "scope": [
        "keyword.control.import.js",
        "keyword.control.import.jsx",
        "keyword.control.import.ts",
        "keyword.control.import.tsx",
        "keyword.control.from.js",
        "keyword.control.from.jsx",
        "keyword.control.from.ts",
        "keyword.control.from.tsx",
        "keyword.control.default.js",
        "keyword.control.default.jsx",
        "keyword.control.default.ts",
        "keyword.control.default.tsx"
      ],
      "settings": {
        "foreground": "#e4baba"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#e4baba"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "CSS Element Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e0a1a4"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#c158eb"
      }
    },
    {
      "name": "CSS Pseudo Class",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "foreground": "#e0a1a4"
      }
    },
    {
      "name": "Attribute Selector",
      "scope": [
        "entity.other.attribute-selector.css",
        "entity.other.attribute-selector.scss",
        "entity.other.attribute-selector.sass"
      ],
      "settings": {
        "foreground": "#41c09a"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "CSS At Rules",
      "scope": [
        "keyword.control.at-rule.media.scss",
